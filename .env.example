# Database
DATABASE_URL=postgresql+asyncpg://username:password@localhost/turn_db
DATABASE_URL_SYNC=postgresql://username:password@localhost/turn_db

# Security
SECRET_KEY=your-secret-key-here-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Environment
ENVIRONMENT=development
DEBUG=True

# AI Services
OPENAI_API_KEY=your-openai-api-key
ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# File Storage
GOOGLE_CLOUD_PROJECT_ID=your-project-id
GOOGLE_CLOUD_STORAGE_BUCKET=turn-file-storage

# Redis (for Celery)
REDIS_URL=redis://localhost:6379/0

# Job Scraping
JOB_SCRAPING_ENABLED=True